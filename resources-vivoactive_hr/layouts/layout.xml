<layout id="WatchFace">
	<label
		id="WeekLabel"
		x="left"
		y="top"
		font="Graphics.FONT_LARGE"
		justification="Graphics.TEXT_JUSTIFY_CENTER"
		color="Graphics.COLOR_WHITE" />
	<label
		id="BatteryLabel"
		x="right"
		y="top"
		font="Graphics.FONT_LARGE"
		justification="Graphics.TEXT_JUSTIFY_CENTER"
		color="Graphics.COLOR_WHITE" />
	<label
		id="DayLabel"
		x="center"
		y="40"
		font="Graphics.FONT_LARGE"
		justification="Graphics.TEXT_JUSTIFY_CENTER"
		color="Graphics.COLOR_WHITE" />
	<label
		id="TimeLabel"
		x="center"
		y="80"
		font="Graphics.FONT_NUMBER_THAI_HOT"
		justification="Graphics.TEXT_JUSTIFY_CENTER"
		color="Graphics.COLOR_WHITE" />
	<label
		id="DateLabel"
		x="center"
		y="bottom"
		font="Graphics.FONT_NUMBER_MILD"
		justification="Graphics.TEXT_JUSTIFY_CENTER"
		color="Graphics.COLOR_WHITE" />
</layout>
